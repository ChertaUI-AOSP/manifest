<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote name="cherta"
          fetch="https://github.com/Project-CHerta"
          revision="25h2"  />

  <remote name="gitlab"
          fetch="https://gitlab.com"
          revision="25h2" />

  <!-- general -->
  <project path="bionic" name="bionic" remote="cherta" />

  <!-- bootable -->
  <project path="bootable/deprecated-ota" name="android_bootable_deprecated-ota" remote="cherta" />
  <project path="bootable/recovery" name="android_bootable_recovery" remote="cherta" />

  <!-- build -->
  <project path="build/make" name="android_build" remote="cherta" >
   <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
   <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
   <linkfile src="core" dest="build/core" />
   <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
   <linkfile src="target" dest="build/target" />
   <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="build/soong" name="android_build_soong" remote="cherta" >
   <linkfile src="root.bp" dest="Android.bp" />
   <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>

  <!-- external -->
  <project path="external/tinycompress" name="android_external_tinycompress" remote="cherta" />

  <!-- frameworks -->
  <project path="frameworks/av" name="android_frameworks_av" remote="cherta" />
  <project path="frameworks/base" name="android_frameworks_base" remote="cherta" />
  <project path="frameworks/native" name="android_frameworks_native" remote="cherta" />

